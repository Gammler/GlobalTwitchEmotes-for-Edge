{
    "author": "Patrick Kr√§mer",
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "description": "Twitch culture wherever you go! This extension replaces all Twitch.tv emote phrases with their actual emoticons.",
    "icons": {
        "16": "logo/16.png",
        "48": "logo/48.png",
        "128": "logo/128.png"
    },
    "Key": "",
    "manifest_version": 2,
    "name": "Global Twitch Emotes",
    "options_page": "data/options/options.html",
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "http://*/",
        "https://*/"
    ],
    "short_name": "GTE",
    "version": "1.1.1021",
    "-ms-preload": {
        "backgroundScript": "backgroundScriptsAPIBridge.js",
        "contentScript": "contentScriptsAPIBridge.js"
    }
}